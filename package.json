{
  "name": "react-quarks",
  "version": "2.0.2",
  "description": "React state management library with async support.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/ncpa0cpl/react-quarks"
  },
  "scripts": {
    "build": "tsc",
    "test:jest": "jest --coverage",
    "test:lint": "eslint .",
    "test:prettier": "prettier --check ./src && prettier --check ./__tests__",
    "test:tsc": "tsc --noEmit",
    "publish:canary": "npm version --no-git-tag-version --preid canary-$(git rev-parse HEAD) prerelease && npm publish --tag canary && git checkout ."
  },
  "author": "Bretner Szymon (ncpa0cpl)",
  "license": "MIT",
  "keywords": [
    "state",
    "management",
    "react",
    "async",
    "asynchronous",
    "promise"
  ],
  "peerDependencies": {
    "react": ">16.8.0"
  },
  "dependencies": {
    "immer": ">9.0.0",
    "lodash": ">4.0.0",
    "luxon": ">2.0.0",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.189",
    "@swc/jest": "^0.2.24",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/node": "^20.1.0",
    "@types/react": "^18.0.27",
    "@types/serialize-javascript": "^5.0.2",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-replace-relative-imports": "^1.0.0",
    "git-hook-tasks": "github:ncpa0cpl/git-hook-tasks",
    "husky": "^8.0.3",
    "immer": "^10.0.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "luxon": "^3.2.1",
    "prettier": "^2.8.3",
    "prettier-plugin-jsdoc": "^0.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "serialize-javascript": "^6.0.1",
    "simple-git": "^3.16.0",
    "typescript": "^5.0.2",
    "use-sync-external-store": "^1.2.0"
  },
  "packageManager": "yarn@1.22.19",
  "sideEffects": false
}