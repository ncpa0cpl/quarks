{
  "name": "react-quarks",
  "version": "2.0.2",
  "description": "React state management library with async support.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/ncpa0cpl/react-quarks"
  },
  "scripts": {
    "build": "tsc",
    "test:jest": "jest --coverage",
    "test:lint": "eslint .",
    "test:prettier": "prettier --check ./src && prettier --check ./__tests__",
    "test:tsc": "tsc --noEmit",
    "publish:canary": "npm version --no-git-tag-version --preid canary-$(git rev-parse HEAD) prerelease && npm publish --tag canary && git checkout ."
  },
  "author": "Bretner Szymon (ncpa0cpl)",
  "license": "MIT",
  "keywords": [
    "state",
    "management",
    "react",
    "async",
    "asynchronous",
    "promise"
  ],
  "peerDependencies": {
    "react": ">16.8.0"
  },
  "dependencies": {
    "immer": ">9.0.0",
    "lodash": ">4.0.0",
    "luxon": ">2.0.0",
    "use-sync-external-store": ">1.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.189",
    "@swc/jest": "^0.2.21",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.172",
    "@types/luxon": "^2.0.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/serialize-javascript": "^5.0.2",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-replace-relative-imports": "^1.0.0",
    "git-hook-tasks": "github:ncpa0cpl/git-hook-tasks",
    "husky": "^8.0.1",
    "immer": "^9.0.14",
    "jest": "^29.3.1",
    "luxon": "^2.4.0",
    "prettier": "^2.6.2",
    "prettier-plugin-jsdoc": "^0.3.38",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^16.14.0",
    "serialize-javascript": "^6.0.0",
    "simple-git": "^3.7.1",
    "typescript": "^4.7.2",
    "use-sync-external-store": "^1.1.0"
  },
  "packageManager": "yarn@1.22.19"
}
