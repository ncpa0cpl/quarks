{
  "name": "react-quarks",
  "version": "2.1.0",
  "description": "React state management library with async support.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/ncpa0cpl/react-quarks"
  },
  "scripts": {
    "build": "node ./scripts/build.mjs && tsc --emitDeclarationOnly",
    "test:jest": "jest --coverage",
    "test:lint": "eslint .",
    "test:prettier": "prettier --check ./src && prettier --check ./__tests__",
    "test:tsc": "tsc --noEmit",
    "publish:canary": "npm version --no-git-tag-version --preid canary-$(git rev-parse HEAD) prerelease && npm publish --tag canary && git checkout ."
  },
  "author": "Bretner Szymon (ncpa0cpl)",
  "license": "MIT",
  "keywords": [
    "state",
    "management",
    "react",
    "async",
    "asynchronous",
    "promise"
  ],
  "peerDependencies": {
    "react": ">16.8.0"
  },
  "dependencies": {
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "use-sync-external-store": "^1.2.2"
  },
  "devDependencies": {
    "@swc/core": "^1.2.189",
    "@swc/jest": "^0.2.24",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.17.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/serialize-javascript": "^5.0.4",
    "@types/use-sync-external-store": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-replace-relative-imports": "^1.0.0",
    "git-hook-tasks": "github:ncpa0cpl/git-hook-tasks",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^3.0.0",
    "prettier-plugin-jsdoc": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "serialize-javascript": "^6.0.2",
    "simple-git": "^3.25.0",
    "typescript": "^5.5.2",
    "use-sync-external-store": "^1.2.2"
  },
  "packageManager": "yarn@1.22.19",
  "sideEffects": false
}